target 'SafeTravel' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SafeTravel
  pod 'Firebase/Core'
  pod 'Firebase/Auth’
pod ‘GoogleMaps’
pod ‘GooglePlaces’

  target 'SafeTravelTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SafeTravelUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
